{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
      "@organisms": ["@next/components/organisms"],
      "@templates": ["./@next/components/templates"],
      "@pages": ["./@next/pages"],
      "@layouts": ["./@next/layouts"]
    }
  },
